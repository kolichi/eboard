<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Inherit the hr appraisal templates -->
        <template id="custom_hr_appraisal_report" inherit_id="hr_appraisal.hr_appraisal_employee_feedback">
